# error
path_doesnt_exist   Error: camino '%s' no existe.

# usage
usage               uso
listing             Listado
	listing(d)          solo directorios
	listing(r)          solo regulares
	listing(h)          solo ocultos
	listing(a)          todos
printing            Impresión
	printing(a)         buffer alternativo (solo en bucle)
	printing(c)         usar colores de LS_COLORS
	printinf(d)         demora (solo en bucle)
	printing(l)         en bucle
sorting             Ordenamiendo
	sorting(d)          directorios primero
	sorting(a)          alfanuméricamente

# count
directories(0)      %u directorios
directories(1)      %u directorio
directories(+)      %u directorios
regular_ones(0)     %u regulares
regular_ones(1)     %u regular
regular_ones(+)     %u regulares

