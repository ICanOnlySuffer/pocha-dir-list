# error
path_doesnt_exist   Camino no existe

# usage
usage               Uso
options             Opciones
printing            Impresión
	printing(a)         buffer alternativo (solo en bucle)
	printing(c)         usar colores de LS_COLORS
	printinf(d)         demora (solo en bucle)
	printing(l)         en bucle
	printing(s)         tamaño
listing             Listado
	listing(d)          solo directorios
	listing(r)          solo regulares
	listing(h)          solo ocultos
	listing(a)          todos
sorting             Ordenamiendo
	sorting(d)          directorios primero
	sorting(n)          por nombre

# count
directories(0)      directorios
directories(1)      directorio
directories(2)      directorios
directories(+)      directorios
regular_ones(0)     regulares
regular_ones(1)     regular
regular_ones(2)     regulares
regular_ones(+)     regulares

