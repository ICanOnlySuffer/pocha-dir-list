# error
path_doesnt_exist   Error: path '%s' doesn't exist.

# usage
usage               Usage
options             Options
printing            Printing
	printing(a)         alternate buffer (on loop only)
	printing(c)         use colors from LS_COLORS
	printing(d)         delay (on loop only)
	printing(l)         on loop
	printing(s)         size
listing             Listing
	listing(d)          only directories
	listing(r)          only regular ones
	listing(h)          only hidden
	listing(a)          all
sorting             Sorting
	sorting(d)          directories first
	sorting(n)          by name

# count
directories(0)      0 directories
directories(1)      1 directory
directories(2)      2 directories
directories(+)      %u directories
regular_ones(0)     0 regular ones
regular_ones(1)     1 regular one
regular_ones(2)     2 regular ones
regular_ones(+)     %u regular ones

